@model IEnumerable<ApartmentApp.Models.UserApartment>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Owner)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Resident)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Apartment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Owner)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Resident)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apartment.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.LastName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UserApartmentID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UserApartmentID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UserApartmentID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
